@model Register

<h1>@ViewData["Title"] </h1>

<hr />

<div>
    <dl class="row">
        <dt class="col-sm-2">
            <label asp-for="@Model.Student.Name" class="form-label"></label>
        </dt>
        <dd class="col-sm-10">
            @Model.Student.Name
        </dd>
        <dt class="col-sm-2">
            <label asp-for="@Model.Course.Subject" class="form-label"></label>
        </dt>
        <dd class="col-sm-10">
            @Model.Course.Subject
        </dd>
        <dt class = "col-sm-2">
            <label asp-for="@Model.TeacherName" class="form-label"></label>
        </dt>
        <dd class = "col-sm-10">
            @Model.TeacherName
        </dd>
        <dt class = "col-sm-2">
            <label asp-for="@Model.Grade" class="form-label"></label>
        </dt>
        <dd class = "col-sm-10">
            @Model.Grade
        </dd>
        <dt class = "col-sm-2">
            <label asp-for="@Model.StartTime" class="form-label"></label>
        </dt>
        <dd class = "col-sm-10">
            @Model.StartTime
        </dd>
        <dt class = "col-sm-2">
            <label asp-for="@Model.Semester" class="form-label"></label>
        </dt>
        <dd class = "col-sm-10">
            @Model.Semester
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Number" class="btn btn-primary">Edit</a> |
    <a asp-action="Index" class="btn btn-success">Back to List</a>
</div>
